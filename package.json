{
  "name": "webfotografia",
  "browserslist": "> 0.5%, last 4 versions, not dead",
  "version": "1.0.0",
  "description": "Web de un grupo aficionado a la fotografía que muestra salidas, imágenes, etc.",
  "scripts": {
    "parcel:desarrollo": "parcel fuente/index.html --dist-dir 'desarrollo'",
    "parcel:produccion": "parcel build fuente/index.html --public-url './' --dist-dir 'produccion'",
    "limpia": "rimraf desarrollo produccion .parcel-cache",
    "des": "npm run parcel:desarrollo",
    "prod": "npm-run-all limpia parcel:produccion"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/avianarios/webfotografia.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/avianarios/webfotografia/issues"
  },
  "homepage": "https://github.com/avianarios/webfotografia#readme",
  "devDependencies": {
    "@parcel/transformer-sass": "^2.8.3",
    "parcel": "^2.8.3",
    "parcel-plugin-imagemin": "^4.0.2",
    "sass": "^1.57.1",
    "rimraf": "^4.1.2",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "bootstrap": "^5.2.3",
    "bootstrap-icons": "^1.10.3",
    "jquery": "^3.6.3"
  }
}
